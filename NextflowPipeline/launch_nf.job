#!/bin/bash
#SBATCH --partition=fast
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=1
#SBATCH --mem-per-cpu=5GB
#SBATCH -c 1
#SBATCH -t 24:00:00

PIPELINE=$1
CONFIG=$2

export JAVA_HOME=/shared/software/conda/envs/nextflow-23.10.1

module load nextflow/23.10.1

nextflow -C ${CONFIG} run ${PIPELINE} -resume